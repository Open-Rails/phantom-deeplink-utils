{
  "name": "phantom-deeplink-utils",
  "version": "0.1.5",
  "author": "OpenRails Inc",
  "description": "Provides a convenient window.solana object and communicates with phantom wallet via a redirect flow rather than direct provider-injection.",
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "exports": {
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js"
  },
  "files": [
    "src",
    "/lib",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "declarations": "tsc -p tsconfig.json",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts' --out-dir 'lib/esm' --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts' --out-dir 'lib/cjs' --source-maps",
    "build:bundles": "cross-env BABEL_ENV=esmBundled rollup -c",
    "build": "yarn clean && yarn build:esm && yarn build:cjs && yarn build:bundles && yarn declarations"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "cross-env": "^7.0.3",
    "json": "^11.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-terser": "^7.0.2",
    "shx": "^0.3.4",
    "ts-node": "^10.7.0",
    "tsc-esm": "^1.0.4",
    "typescript": "^4.6.3",
    "typescript-esm": "^2.0.0"
  }
}
