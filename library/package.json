{
  "name": "rollup-hello-world2",
  "version": "0.1.19",
  "description": "silly description",
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "exports": {
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js"
  },
  "files": [
    "lib",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf lib",
    "declarations": "tsc -p tsconfig.json",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts' --out-dir 'lib/esm' --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts' --out-dir 'lib/cjs' --source-maps",
    "build:bundles": "cross-env BABEL_ENV=esmBundled rollup -c",
    "build": "yarn clean && rollup -c && yarn postbuild",
    "postbuild": "echo {\"type\":\"commonjs\", \"types\":\"../types/index.d.ts\"} | json > lib/cjs/package.json && echo {\"type\":\"module\", \"types\":\"../types/index.d.ts\"} | json > lib/esm/package.json"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "cross-env": "^7.0.3",
    "json": "^11.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "shx": "^0.3.4",
    "ts-node": "^10.7.0",
    "tsc-esm": "^1.0.4",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3",
    "typescript-esm": "^2.0.0"
  }
}
